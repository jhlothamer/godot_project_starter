[global]

monitor=false
transition_mgr_default_speed=0.5
screenshot_mgr_max_images=1000
screenshot_mgr_resize_factor=1.0
screenshot_mgr_toggle_action_name="toggle_screenshots"
screenshot_mgr_frame_delay=0.1

